{
  "private": true,
  "version": "2020.4.0",
  "scripts": {
    "build:client": "rm -rf dist && rollup -c",
    "check": "npm run eslint && npm run tsc && npm run prettier && npm run depcheck",
    "client": "rollup -c -w",
    "depcheck": "depcheck --ignore-dirs dist --ignores=@types/*,@hot-loader/react-dom,env-cmd,parcel,ts-node,concurrently,tsconfig-paths,webpack-cli,@mdx-js/parcel-plugin-mdx,mace-types,react-mace",
    "eslint": "eslint --fix server/ example/ client/ types/ --max-warnings=0 --ext .ts,.tsx",
    "ncu": "ncu",
    "server": "nodemon",
    "start": "concurrently -n server,client,example --handle-input \"npm run server\" \"npm run client\" \"parcel example/index.html\"",
    "tsc": "tsc --noEmit",
    "prettier": "prettier --write client/ example/ server/ types/ .vscode/ *.json *.js && prettier-package-json --write"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@types/uuid": "^7.0.2",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-easy-ws": "https://github.com/cs125-illinois/koa-easy-ws.git",
    "koa-router": "^8.0.8",
    "lodash": "^4.17.15",
    "query-string": "^6.12.1",
    "reconnecting-websocket": "^4.4.0",
    "runtypes": "^4.2.0",
    "uuid": "^7.0.3",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@mdx-js/parcel-plugin-mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.0",
    "@types/koa__cors": "^3.0.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/url-join": "^4.0.0",
    "@types/webpack-env": "^1.15.1",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "ace-builds": "^1.4.11",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.1.0",
    "depcheck": "^0.9.2",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^2.0.3",
    "npm-check-updates": "^4.1.2",
    "parcel": "^1.12.4",
    "prettier": "^2.0.4",
    "prettier-package-json": "^2.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-app-polyfill": "^1.0.6",
    "react-children-utilities": "^2.0.13",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-syntax-highlighter": "^12.2.1",
    "rollup": "^2.6.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom",
    "react-mace": "./client/dist/index.cjs.js"
  },
  "nodemonConfig": {
    "watch": [
      "package.json",
      "package-lock.json",
      "server/index.ts"
    ],
    "exec": "env-cmd ts-node -r tsconfig-paths/register server/index.ts"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
